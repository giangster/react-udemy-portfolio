SS = \"https://spotify-api-wrapper.appspot.com\";\n    fetch(`${API_ADDRESS}/artist/${artistQuery}`)\n      .then(response => response.json())\n      .then(responseData => {\n        if (responseData.artists.total > 0) {\n          const artist = responseData.artists.items[0];\n          this.setState({ artist });\n          fetch(`${API_ADDRESS}/artist/${artist.id}/top-tracks`)\n            .then(response => response.json())\n            .then(responseData => {\n              this.setState({ tracks: responseData.tracks });\n            })\n            .catch(error => alert(error.message));\n        }\n      })\n      .catch(error => alert(error.message));\n  };\n\n  render() {\n    return (\n      <div>\n        <h2>Music Master</h2>\n        <Search searchArtist={this.searchArtist} />\n        <hr />\n        <Artist artist={this.state.artist} />\n        <Tracks tracks={this.state.tracks} />\n      </div>\n    );\n  }\n}\n\nexport default App;\n"},"lineCount":null}},"error":null,"hash":"54ec22e9347f2c7c4f934cd30bb155dd","cacheData":{"env":{}}}